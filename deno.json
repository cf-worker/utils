{
  "name": "@cf-worker/utils",
  "version": "0.2.9",
  "exports": "./mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@^0.226.0"
  },
  "publish": {
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "compilerOptions": {
    "types": [
      "./types.d.ts"
    ]
  },
  "tasks": {
    "test": "deno test",
    "dry-publish": "deno publish --dry-run --allow-dirty",
    "publish": "deno publish",
    "coverage": "deno test --coverage; deno coverage --html; open coverage/html/index.html"
  }
}
